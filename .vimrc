set nocompatible
set selection=old
set hls
set ignorecase
set smartcase
set tabstop=4
set shiftwidth=4
set softtabstop=4
set expandtab
set number
set mouse=a
syntax on
set timeout
set ttimeout
set timeoutlen=300
lnoremap jk <ESC>
cnoremap jk <ESC>
noremap jk <ESC>
inoremap jk <ESC>
noremap <F7> :set hls!<CR>
